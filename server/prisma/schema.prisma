generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

enum TaskStatus {
  FINISHED
  PROCESS
  FREE
  EXPIRED
}

model Task {
  id Int @default(autoincrement()) @id
  title String
  status TaskStatus @default(FREE)
  deadline DateTime
  
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
